ossieName = ex.simpleGPS
bindir = $(prefix)/dev/devices/ex/simpleGPS/cpp/
bin_PROGRAMS = simpleGPS

xmldir = $(prefix)/dev/devices/ex/simpleGPS/
dist_xml_DATA = ../simpleGPS.scd.xml ../simpleGPS.prf.xml ../simpleGPS.spd.xml
ACLOCAL_AMFLAGS = -I m4 -I${OSSIEHOME}/share/aclocal/ossie
AUTOMAKE_OPTIONS = subdir-objects



distclean-local:
	rm -rf m4
	rm -f config.*
	rm -rf autom4te.cache
	rm -f acinclude.m4
	rm -f aclocal.m4
	rm -f configure
	rm -f depcomp
	rm -f install-sh
	rm -f ltmain.sh
	rm -f Makefile.in
	rm -f missing
	rm -rf .deps


